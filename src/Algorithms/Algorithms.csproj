<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Algorithms</RootNamespace>
    <AssemblyName>Algorithms</AssemblyName>
    <PackageId>BurkhardKellerTree</PackageId>
    <Description>Burkhard-Keller tree implementation with string distance algorithms including Levenshtein, Damerau-Levenshtein, Double Metaphone, and StringSift3.</Description>
